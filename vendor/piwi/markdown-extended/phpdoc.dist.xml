<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>PHP Markdown Extended</title>
    <parser>
        <target>phpdoc-2/build</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>md</extension>
            <extension></extension>
        </extensions>
        <default-package-name>mde</default-package-name>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>phpdoc-2</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>{APP_ROOT}/tmp/{DATE}.log</default>
            <errors>{APP_ROOT}/tmp/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>src/SplClassLoader.php</ignore>
    </files>
</phpdoc>